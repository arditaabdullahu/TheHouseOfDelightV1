
@{
    ViewData["Title"] = "AddNew";
}

<h1>AddNew</h1>

<form method="post" action="/Admin1/AddNew">
   
    <div class="form-group  col-md-12">
        <label for="Name">Name</label>
        <input type="text" class="form-control" id="Name" name="Name" value="">
    </div>
    <div class="form-group  col-md-12">
        <label for="Description">Description</label>
        <input type="text" class="form-control" id="Description" name="Description" value="">
    </div>
    <div class="form-group col-md-2 ">
        <label for="PrepTime">Prep Time</label>
        <input type="text" class="form-control" id="PrepTime" name="PrepTime" value="">
    </div>
    <div class="form-group col-md-2">
        <label for="CookTime">Cook Time</label>
        <input type="text" class="form-control" id="CookTime" name="CookTime" value="">
    </div>
    <div class="form-group col-md-2">
        <label for="TotatTime">Total Time</label>
        <input type="text" class="form-control" id="TotatTime" name="TotatTime" value="">
    </div>
    <div class="form-group col-md-2">
        <label for="Servings">Servings</label>
        <input type="text" class="form-control" id="Servings" name="Servings" value="">
    </div>


    <input type="submit" class="btn btn-primary " value="Save" />
    <a href="/admin1/Food" class="btn btn-primary ">Back</a>
</form>
@model List<TheHouseOfDelightV1.DbModel.Types>
@{
    ViewData["Title"] = "Types";
}

<h1>Types</h1>


<a href="/admin1/AddNewTypes/" class="btn btn-primary">Add New Types</a>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Edit|Delete</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>

                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Description</td>

                <td>
                    <a href="/admin1/EditTypes/@item.ID" class="btn btn-primary">Edit</a>
                    <button type="button" class="btn btn-primary"> Delete</button>

                </td>
            </tr>

        }

    </tbody>



</table>